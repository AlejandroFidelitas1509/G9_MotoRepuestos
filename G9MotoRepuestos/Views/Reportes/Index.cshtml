@model IEnumerable<G9MotoRepuestos.Models.Reportes>

@{
    ViewData["Title"] = " Reportes -  Moto Repuestos Rojas";
}

<H2>Reportes</H2>

<a name=""
   id=""
   class="btn btn-primary"
   asp-action="Crear"
   role="button">Generar Reporte</a>

<div class="mt-3">  
    <table class="table">
     <thead>
          <tr>
                <th>Id</th>
                <th>Fecha Inicio</th>
                <th>Fecha Fin</th>
                <th>Acciones</th>
          </tr>
     </thead>
          <tbody>
                @foreach (Reportes itemDato in Model)
                {
    
                 <tr class="">
    
                      <td>@itemDato.</td>
                      <td>@itemDato.</td>
                      <td>@itemDato.</td>
                      <td>
                            <a name=""
                            id=""
                            class="btn btn-primary"
                            asp-action="Detalles"
                            asp-route-id="@itemDato."
                            role="button">Detalles</a>
                      </td>
                 </tr>
                }
          </tbody>
     </table>

</div>

